{
  "manifest_version": 3,
  "name": "QuickTab",
  "version": "1.3",
  "description": "Quickly access your open tabs with a keyboard shortcut",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Space"
      },
      "description": "Open QuickTab menu"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "QuickTab@myoboku.net",
      "strict_min_version": "109.0"
    }
  }
}
